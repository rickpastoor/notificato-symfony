	"homepage": "https://github.com/wrep/notificato-symfony",
	"license": "MIT",
	"authors": [
		{
			"name": "Rick Pastoor",
			"email": "rick@wrep.nl"
		},
		{
			"name": "Mathijs Kadijk",
			"email": "mathijs@wrep.nl"
		}
	],
	"require": {
		"php": ">=5.3.3",
		"symfony/framework-bundle": "^2.7 || ^3.0",
		"wrep/notificato": "~1.0"
	},
	"replace": {
		"wrep/notificare-symfony": "*"
